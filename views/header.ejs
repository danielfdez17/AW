<head>
    <link rel="stylesheet" href="/css/header.css">
</head>
<header class="mx-0 d-flex justify-content-between align-items-center bg-warning sticky-top">
    <a href="http://www.ucm.es" target="_blank" rel="noopener noreferrer">
        <img src="/resources/logo.png" alt="Logotipo" class="ms-5 logo">
    </a>

    <div class="d-flex align-items-center gap-2 me-3">

        <button class="btn border-0 bg-transparent">
            <span class="material-symbols-rounded fs-2 d-flex align-items-center">
                notifications
            </span>
        </button>
         
        <button class="btn border-0 bg-transparent d-flex align-items-center inicio" type="button" data-bs-toggle="modal" data-bs-target="#modalLogin">
            <span class="fs-4">
                Log In
            </span>
        </button>

        <div class="modal fade" id="modalLogin" tabindex="-1" aria-labelledby="modalLoginLabel" aria-hidden="true">
            <div class="modal-dialog">
              <div class="modal-content">
                <div class="modal-header">
                  <h1 class="modal-title fs-5" id="exampleModalLabel">Modal title</h1>
                  <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                  ...
                </div>
                <div class="modal-footer">
                  <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                  <button type="button" class="btn btn-primary">Save changes</button>
                </div>
              </div>
            </div>
          </div>
          
     
         <button class="btn border-0 bg-black d-flex align-items-center registrar">
            <span class="fs-4">
                Sign Up
            </span>
         </button>
     
         <button class="btn border-0 bg-transparent d-flex align-items-center">
            <span class="material-symbols-rounded fs-2">
                logout
            </span>
         </button>
     
         <button class="btn border-0 bg-transparent d-flex align-items-center">
             <span class="material-symbols-rounded fs-2">
                settings
             </span>
         </button>
    </div>


</header>