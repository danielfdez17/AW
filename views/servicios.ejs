<!DOCTYPE html>
<html lang="es">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Todos los servicios</title>
    <link rel="stylesheet" href="/css/card-servicio.css">
    <%- include("../public/html/head.html") %>
</head>

<body>
    <%- include("./fragments/navbar.ejs") %>
    <div class="container">
        <h1 class="font-weight-bold text-center">Listado con todos los servicios creados</h1>
        <div class="d-flex justify-content-center">
            <% servicios.forEach(servicio=> { %>
                <%- include("./fragments/card-servicio.ejs", {servicio: servicio}) %>
            <% }) %>
        </div>
    </div>

</body>

</html>